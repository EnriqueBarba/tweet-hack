<h1>hello {{currentUser.name}}</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Password</th>
    </tr>
  </thead>

  <tbody>
    {{#each users as |user|}}
      <tr>
        <td>{{user.name}}</td>
        <td>{{user.email}}</td>
        <td>{{user.password}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
